    "@vercel/blob": "^2.2.0",
    "bcryptjs": "^2.4.3",
    "chart.js": "^4.5.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "docx-preview": "^0.3.7",
    "exceljs": "^4.4.0",
